{
  "name": "warehouse-revenue-management-backend",
  "private": true,
  "type": "module",
  "version": "0.1.0",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/src/server.js",
    "migrate:deploy": "prisma migrate deploy --schema=./prisma/schema.prisma",
    "db:push": "prisma db push --schema=./prisma/schema.prisma",
    "db:studio": "prisma studio --schema=./prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./prisma/schema.prisma",
    "db:reset": "prisma migrate reset --schema=./prisma/schema.prisma",
    "db:seed": "tsx prisma/seed.ts",
    "backup": "node dist/scripts/backup.js",
    "restore": "node dist/scripts/restore.js",
    "postbuild": "mkdir -p dist/src/templates && cp -R src/templates/* dist/src/templates/",
    "postinstall": ""
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/multipart": "^9.0.3",
    "@fastify/static": "^8.2.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/adapter-pg": "^6.14.0",
    "@prisma/client": "^6.14.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "bcrypt": "^6.0.0",
    "csv-stringify": "^6.6.0",
    "docxtemplater": "^3.67.6",
    "dotenv": "^17.2.1",
    "exceljs": "^4.4.0",
    "fastify": "^5.5.0",
    "jsonwebtoken": "^9.0.3",
    "mime-types": "^3.0.1",
    "pg": "^8.16.3",
    "pizzip": "^3.2.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/mime-types": "^3.0.1",
    "@types/node": "^24.2.1",
    "prisma": "^6.14.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
